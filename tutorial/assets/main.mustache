<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="{{name}}-Creative, build customizable HTML templates using your own tools and workflow.">

    <title>{{name}} Web Developer Toolkit</title>

    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="{{assets.css.bootstrap}}">

    <!-- Documentation extra CSS -->
    <link rel="stylesheet" href="{{assets.css.docs}}">

    <!-- Baliho theme -->
    <link rel="stylesheet" href="{{assets.css.theme}}">

    <!-- Favicon -->
    <link rel="icon" href="{{assets.img.favicon}}">

    <script src="{{assets.js.jquery}}"></script>
  <head>

  <body>
    <!-- Nav bar -->
    {{> navbar}}

    <!-- Page layout -->
    {{#request}}
      {{#ifpage.index}}      {{> index}}      {{/ifpage.index}}
      {{#ifpage.assets}}     {{> assets}}     {{/ifpage.assets}}
      {{#ifpage.urls}}       {{> urls}}       {{/ifpage.urls}}
      {{#ifpage.sampledata}} {{> sampledata}} {{/ifpage.sampledata}}
      {{#ifpage.test}}       {{> test}}       {{/ifpage.test}}
      {{#ifpage.config}}     {{> config}}     {{/ifpage.config}}
      {{#ifpage.notfound}}   {{> notfound}}   {{/ifpage.notfound}}
    {{/request}}

    <!-- Footer -->
    {{> footer}}

    <!-- Javascript -->
    <script src="{{assets.js.sidenav}}"></script>
    <script src="{{assets.js.bootstrap}}"></script>
    <script src="{{assets.js.docs}}"></script>

    {{{$tests}}}
  </body>

</html>

